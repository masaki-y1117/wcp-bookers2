<%= render 'books/list', book: @book, user: @user %>
<h1>Users</h1>
<table>
  <th>
    <tr>image</tr>
    <tr>name</tr>
  </th>
  <% @users.each do |user| %>
  <tr>
    <td><%= attachment_image_tag(user, :profile_image, :fill,50,50,fallback: "no-image.jpg") %></td>
    <td><%= user.name %></td>
    <td><%= link_to "Show", user_path(user.id) %></td>
  </tr>
  <% end %>
</table>